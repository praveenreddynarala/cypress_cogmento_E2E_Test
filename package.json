    "@types/faker": "^5.1.4",
    "chai": "^4.2.0",
    "cypress": "^5.6.0",
    "cypress-multi-reporters": "^1.4.0",
    "cypress-xpath": "^1.6.0",
    "json-server": "^0.16.2",
    "mocha": "^8.2.1",
    "mochawesome": "^6.2.1"
  },
  "dependencies": {
    "@types/json-server": "^0.14.2",
    "faker": "^5.1.0",
    "i": "^0.3.6",
    "jason-server": "^3.0.0",
    "mocha-jenkins-reporter": "^0.4.5",
    "mochawesome-merge": "^4.2.0",
    "typescript": "^4.0.5"
  }
}
